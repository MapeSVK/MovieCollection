<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="mainWindow" onMouseClicked="#mainWindowClick" prefHeight="613.0" prefWidth="1141.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="moviecollection.gui.MainViewController">
   <children>
      <TableView fx:id="categoryMoviesTableView" layoutX="194.0" layoutY="65.0" onMouseClicked="#mICClick" prefHeight="450.0" prefWidth="463.0">
        <columns>
          <TableColumn prefWidth="196.0" text="Title" />
          <TableColumn prefWidth="82.0" text="My Rating" />
            <TableColumn prefWidth="95.0" text="IMDB Rating" />
            <TableColumn prefWidth="89.0" text="Last Viewed" />
        </columns>
      </TableView>
      <TableView fx:id="allMoviesTableView" layoutX="672.0" layoutY="66.0" onMouseClicked="#mClick" prefHeight="450.0" prefWidth="455.0">
        <columns>
          <TableColumn prefWidth="182.0" text="Title" />
          <TableColumn prefWidth="87.0" text="My Rating" />
            <TableColumn prefWidth="96.0" text="IMDB Rating" />
            <TableColumn prefWidth="89.0" text="Last Viewed" />
        </columns>
      </TableView>
      <ListView fx:id="categoryListView" layoutX="18.0" layoutY="66.0" onMouseClicked="#categoryClick" prefHeight="450.0" prefWidth="160.0" />
      <TextField fx:id="searchTextField" layoutX="888.0" layoutY="24.0" />
      <Button fx:id="searchButton" layoutX="1066.0" layoutY="24.0" mnemonicParsing="false" text="Search" />
      <Button fx:id="addCategoryButton" layoutX="25.0" layoutY="532.0" mnemonicParsing="false" onAction="#addNewCategoryClick" prefHeight="21.0" prefWidth="48.0" text="Add" />
      <Button fx:id="deleteCategoryButton" layoutX="94.0" layoutY="531.0" mnemonicParsing="false" onAction="#deleteCategoryClick" prefHeight="30.0" prefWidth="60.0" text="Delete" />
      <Button fx:id="editMovieButton" layoutX="764.0" layoutY="533.0" mnemonicParsing="false" onAction="#editMovieClick" text="Edit" />
      <Button fx:id="deleteMovieButton" layoutX="830.0" layoutY="533.0" mnemonicParsing="false" onAction="#deleteMovieClick" text="Delete" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="22.0" layoutY="37.0" prefHeight="17.0" prefWidth="156.0" text="Categories">
         <font>
            <Font name="Arial Bold" size="17.0" />
         </font>
      </Label>
      <VBox fx:id="vBox" accessibleRole="BUTTON" disable="true" layoutX="134.0" layoutY="120.0" prefHeight="53.0" prefWidth="100.0" style="-fx-border-color: #000000; -fx-background-color: #e8ebed;" visible="false">
         <children>
            <Label fx:id="addM" accessibleRole="BUTTON" onMouseClicked="#clickAddM" onMouseEntered="#enterAddM" onMouseExited="#exitAddM" prefHeight="22.0" prefWidth="201.0" text="Add Movie" />
         </children>
      </VBox>
      <VBox fx:id="vBoxCat" accessibleRole="BUTTON" disable="true" layoutX="134.0" layoutY="290.0" prefHeight="53.0" prefWidth="100.0" style="-fx-border-color: #000000; -fx-background-color: #e8ebed;" visible="false">
         <children>
            <Label fx:id="addC" accessibleRole="BUTTON" onMouseEntered="#enterAddC" onMouseExited="#exitAddC" prefHeight="22.0" prefWidth="201.0" text="Add Categories  &gt;" />
         </children>
      </VBox>
   </children>
</AnchorPane>
